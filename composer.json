  "description": "Simple PHP CLI (Command Line Interface) framework",
  "type": "library",
  "homepage": "https://github.com/JesusValera/minicli",
  "license": "MIT",
  "keywords": [
    "cli",
    "framework",
    "php"
  ],
  "authors": [
    {
      "name": "Jesus Valera Reales",
      "email": "jesus1902@outlook.com"
    }
  ],
  "autoload": {
    "psr-4": {
